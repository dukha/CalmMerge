<!DOCTYPE html>
<html>
<head>
  <title>Prototype 1</title>
  <!-- <%= stylesheet_link_tag :all %> -->
	<%=javascript_include_tag "jquery-1.4.4.js" %> <!--"http://ajax.googleapis.com/ajax/libs/jquery/1.4.4/jquery.min.js" -->
  <%=javascript_include_tag  "jquery-ui-1.8.6.custom.min.js" %><!--"http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.6/jquery-ui.min.js" -->
  <%=javascript_include_tag "application.js"%>
	<%=javascript_include_tag "jquery.tablesorter.js" %>
	<%=javascript_include_tag "rails.js"%>
	<script type="text/javascript">
      
  </script>
	
	<%= stylesheet_link_tag  'google.docs.css','application.css', 'formtastic.css', 'formtastic_changes.css', 'ui-lightness/jquery-ui-1.8.6.custom.css',
                                             'custom.css', 'donotedit/calm.css' %> 
  <!-- can't do this on heroku: %= stylesheet_link_tag  'google.docs.css','application.css', 'formtastic.css', 'formtastic_changes.css', 'ui-lightness/jquery-ui-1.8.6.custom.css',
                                             'custom.css', 'donotedit/calm.css', :cache => 'base' %  --> 
  <!-- style media="screen" type="text/css">  div, td.maindisplay  { border-style: none; border-width:1px; border-color:red; } td{border-color:green;} </style -->
	
  <%= csrf_meta_tag %>
</head>
<body>
  <div id = "bigcontainer" style= "">
      
    <div id="appheader" style= "border-color:red;">
      <%= render 'layouts/header' %>
    </div>
	  

    
   <table id ="appmaintable" >
      <tr>
        <td id="appmenutd" class="maindisplay" >
        
          <%= render 'layouts/menu' %>
					
        </td>
        <!-- td id="appmaintd" class="maindisplay"  -->
          
        <!-- /td -->
				<td id="appmaintd"  class="maindisplay">
					<div id="container">
			      <% flash.each do |name, msg| %>
			        <%= content_tag :div, msg, :id => "flash_#{name}" %>
			      <% end %>
						
						 
			
			      <%= content_tag :h1, yield(:title) if show_title? %>
			      <%= yield %>
       </div></td>
      </tr>
    </table>
      <div id="appfooter"  style= "">
        <%= render 'layouts/footer' %>
      </div>
  </div>
</body>
</html>
